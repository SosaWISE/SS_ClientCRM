{
  "strict": true,
  "node": false,
  "browser": false,

  "onevar": false,
  "laxcomma": true,

  "eqeqeq": true,
  "eqnull": true,
  "immed": true,
  "undef": true,
  "unused": true,
  "latedef": true,
  "curly": true,
  "trailing": true,

  "smarttabs": true,

  "predef": [
    "Intl",
    "require",
    "define",
    "alert",
    "console",

    "FileReader",
    "HTMLInputElement",
    "HTMLAnchorElement",
    "HTMLDivElement",
    "MouseEvent",
    "ArrayBuffer",
    "Uint8Array",
    "Blob",
    "FormData",
    "atob",
    "localStorage",
    "navigator",
    "clearInterval",
    "setInterval",
    "clearTimeout",
    "setTimeout",
    "location",
    "screen",
    "document",
    "window"
  ]
}
