div(class="acct-item acct-systemtest",data-bind="toggle:{'may-reload':mayReload}")
  div(class="two-way",style="margin-bottom: 10px;")
    h2 Two-Way Check
    ul(class="actions")
      li
        a(class="btn",tabindex="0",data-bind="cmd:cmdInitTwoWay") Initiate Two-Way Test
      li
        a(class="btn",data-bind="tcmd:tcmdServiceState")
    div(data-bind="if:twoWayTestData()&&!twoWayTestData().ConfirmedOn")
      span Started On:&nbsp;
      span(data-bind="datetimetext:twoWayTestData().TwoWayTestStartedOn")
      br
      ul(class="edit-form")
        li
          span Confirmation:&nbsp;
          input(type="text",style="width:200px;margin-right:5px;"
            data-bind="ukov:confirmedBy,value:confirmedBy,valueUpdate:'keyup'")
          a(class="btn",tabindex="0",data-bind="cmd:cmdSaveConfirmation") Save
    div(data-bind="if:twoWayTestData()&&twoWayTestData().ConfirmedOn")
      <!-- ko with:twoWayTestData -->
      span Started On:&nbsp;
      span(data-bind="datetimetext:TwoWayTestStartedOn")
      br
      span Confirmed On:&nbsp;
      span(data-bind="datetimetext:ConfirmedOn")
      br
      span Confirmation:&nbsp;
      span(data-bind="text:ConfirmedBy")
      <!-- /ko -->

  div(class="active-tests")
    h2 Active Tests
    ul(class="actions")
      li
        a(class="btn",tabindex="0",data-bind="cmd:cmdRefreshGrid") Refresh Grid
      li
        a(class="btn",tabindex="0",data-bind="cmd:cmdClearAllTests") Clear All Tests
    div(class="grid-wrap rv",style="height: 100px; position: relative;")
      div(class="slickgrid",data-bind="slickgrid:gvm")

  div(data-bind="template:{name:signalHistoryVm.viewTmpl,data:signalHistoryVm}")

  div(data-bind="spinner:active()&&!loaded(),spinner-rgb:'black'")
