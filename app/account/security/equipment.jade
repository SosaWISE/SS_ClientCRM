div(class="acct-item acct-equipment",data-bind="toggle:{'may-reload':mayReload}")
  table(class="fh-wrap")
    tr
      td(class="fh-header")
        ul(class="actions left cf")
          li
            a(class="btn small add-btn",tabindex="0",data-bind="cmd:cmdAddByBarcode") Add By Barcode
          li
            a(class="btn small add-btn",tabindex="0",data-bind="cmd:cmdAddByPart") Add By Part
          li
            a(class="btn small add-btn",tabindex="0",data-bind="cmd:cmdAddExistingEquipment") Add Existing Equipment
        div
          |Total Points:&nbsp;
          span(data-bind="text:gvm.totalPoints")
    tr
      td(class="fh-body")
        div(class="fh-contents")
          div(class="slickgrid",data-bind="slickgrid:gvm")

div(class="layers", data-bind="template:{name:layersVm.viewTmpl,data:layersVm}")
