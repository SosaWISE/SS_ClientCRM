div(class="acct-item acct-clist-systemdetails")
  table(class="fh-wrap")
    tr
      td(class="fh-header")
        div(class="col")
          div(class="section",data-bind="cmd:cmdFindRep,toggle:{empty:!repModel()}")
            span(class="title") Technician
            div(class="contents",data-bind="if:repModel")
              div(data-bind="template:{data:repModel,name:'tmpl-acct-default-rep_info'}")

    tr
      td(class="fh-body")
        div(class="fh-contents")

  div(data-bind="if:active()&&!loaded()")
    div(class="spinner-wrap black", data-bind="template:'tmpl-spinner'")
