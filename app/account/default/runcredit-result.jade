h2(class="title") Found Credit Result!
div(class="runcredit-result line-top")
  ul
    li
      span(class="label") Customer #:
      span(data-bind="text:lead.CustomerMasterFileId")
    li
      span(class="label") Credit Group:
      span(data-bind="text:result.CreditGroup")
    li
      span(class="label") Lead ID:
      span(data-bind="text:result.LeadId")
    li
      span(class="label") Account ID:
      span [account not yet created]
    li(style="background-color: red;padding: 10px;font-weight: bold;text-transform: uppercase;",data-bind="visible: result.IsTakeOver")
      span(class="label") Take Over:
      span(data-bind="text:result.TakeOverFromCompanyName")

div(class="actions")
  span(class="btn-bg", tabindex="0", data-bind="cmd:$parent.close") Ok

