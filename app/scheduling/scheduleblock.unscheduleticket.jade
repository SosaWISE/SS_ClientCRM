ul(class="edit-form")
	li
		span(class="label") Name
		input(type="text" disabled data-bind="ukov:data.TicketAccountName, value:data.TicketAccountName,valueUpdate:'keyup'")
	li
		span(class="label") Ticket Number
		input(type="text" disabled data-bind="ukov:data.TicketNumber, value:data.TicketNumber,valueUpdate:'keyup'")
	li
		span(class="label") Ticket Type
		input(type="text" disabled data-bind="ukov:data.TicketType, value:data.TicketType,valueUpdate:'keyup'")
	li
		span(class="label") Zipcode
		input(type="text" disabled data-bind="ukov:data.TicketZipcode, value:data.TicketZipcode,valueUpdate:'keyup'")
	li
		span(class="label") General Notes
		textarea(rows="5" data-bind="ukov:data.TicketNotes, value:data.TicketNotes,valueUpdate:'keyup'")		
div(class="actions")
	span(class="btn-bg", tabindex="0", data-bind="cmd: cmdUnscheduleTicket") Unschedule This Ticket
	
	