div(class="ticket-editor")
  div(data-bind="spinner:loading,spinner-rgb:'white'")
ul(class="edit-form")
  li
    span(class="label") Account ID 
    input(type="text" data-bind="ukov:data.AccountId, value:data.AccountId,valueUpdate:'keyup'")  
  li    
    span(class="label") Ticket Type
    div(data-bind="cvm:data.ticketTypeCvm")
  li
    span(class="label") General Note 
    textarea(rows="5" data-bind="ukov:data.Notes, value:data.Notes,valueUpdate:'keyup'")
  li
    span(class="label")  Monitoring Station Ticket#
    input(type="text" data-bind="ukov:data.MonitoringStationNo, value:data.MonitoringStationNo,valueUpdate:'keyup'")    
  div
      ul(class="actions")        
        li
          a(class="btn",tabindex="0",data-bind="cmd:cmdUserAccount") User Account
        li
          a(class="btn",tabindex="0",data-bind="cmd:cmdSaveAndSchedule") Save and Schedule 	
