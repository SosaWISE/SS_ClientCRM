table(class="fh-wrap")
  tr
    td(class="fh-header relative")
      ul(class="edit-form")
        li(style="margin: 10px;")
          div(class="header_left")
            ul(class="header_ul" style="color: white;")
              li
                span(class="label") Location Type
                div(data-bind="cvm:data.LocationTypeCvm")         
              li
                span(class="label") Location
                div(data-bind="cvm:data.LocationCvm")                   
              li
                span(class="label") Enter Barcode           
                input(type="text")
                //-input(type="text" placeholder="" data-bind="ukov:data.productBarcodeID, value:data.productBarcodeID,valueUpdate:'afterkeydown',select:focusFirst, event: { keyup: processBarcode}")              
  tr
    td(class="fh-body")
      div(class="fh-contents")
        div(class="slickgrid", style="background:white;margin:10px;color:black;", data-bind="slickgrid:itemListGvm")
  tr
    td(class="fh-contents")
      input(type="submit" class="btn-bg" style="margin: 10px; width: 25%;" data-bind="cmd: cmdDisplay")
  div(data-bind="template:{name:layersVm.viewTmpl,data:layersVm}")       
        