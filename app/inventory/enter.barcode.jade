div(class="barcode_wrapper")
  ul(class="edit-form")
    li
      label PO#: 
      label(data-bind="text: PurchaseOrderId")
    li
      label Packing Slip#: 
      label(data-bind="text: PackingSlipID")
    li
      label Count#  
      label(data-bind="text: Count")
    li
      label Enter#  
      label(data-bind="text: EnteredBarcode")
    li
      label Barcode 
      input(type="text")
  button(type="button" class="btn-enterBarcode" style="width: 100%;") Done Entering Barcodes   
  div(class="actions")    
      span(class="btn-bg", tabindex="0", data-bind="cmd:clickClose") Close